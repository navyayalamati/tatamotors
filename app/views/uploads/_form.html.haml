= form_for @upload, :multipart =>true, :class => "form-horizontal" do |f|
  .col-sm-8
    .row
      .form-group
        = f.label :file_name,:class => 'col-sm-3 control-label'
        .col-sm-6
          = f.text_field :file_name,:class => 'form-control'
    %br
    .row
      .form-group
        = f.label :image,:class => 'col-sm-3 control-label'
        .col-sm-6
          = f.file_field :file_type,:class => 'form-control'
    %br
    .row
      .form-group
        = f.label :file,:class => 'col-sm-3 control-label'
        .col-sm-6
          = f.file_field :file
    .row
      .col-sm-6.col-sm-offset-5
        = f.submit "submit",:class => 'btn btn-primary'
